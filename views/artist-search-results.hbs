<div class='grid-container'>
  {{#each result}}

    <div>
      <div>
        <img class='album-image' src='{{images.0.url}}' alt='image' />
      </div>

      <div class='name-frame'>
        <h3 class='artist-name'>{{name}}</h3>

        <a class='artist-btn' href='/albums/{{id}}'>
          View Albums
        </a>
      </div>

    </div>

  {{/each}}
</div>